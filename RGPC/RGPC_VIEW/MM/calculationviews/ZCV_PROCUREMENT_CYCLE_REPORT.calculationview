    <calculationView xsi:type="Calculation:AggregationView" id="Days_Calculation" filterExpressionLanguage="COLUMN_ENGINE">
      <descriptions/>
      <viewAttributes>
        <viewAttribute id="RFQ_Creation_Date"/>
        <viewAttribute id="Commer_Eval_Date"/>
        <viewAttribute id="RFQ_Number"/>
        <viewAttribute id="Tech_Eval_Date"/>
        <viewAttribute id="PR_Approved_Date"/>
        <viewAttribute id="BNFPO" keepFlag="false"/>
        <viewAttribute id="BANFN" keepFlag="false"/>
        <viewAttribute id="PO_Type"/>
        <viewAttribute id="PO_Approval_Date"/>
        <viewAttribute id="PO_Creation_Date"/>
        <viewAttribute id="First_Delivery_Date"/>
        <viewAttribute id="BELNR"/>
        <viewAttribute id="EBELN"/>
        <viewAttribute id="EBELP"/>
        <viewAttribute id="TXZ01" transparentFilter="false"/>
        <viewAttribute id="Material_Service" transparentFilter="false"/>
        <viewAttribute id="Vendor_Type" transparentFilter="false"/>
        <viewAttribute id="EKNAM" transparentFilter="false"/>
        <viewAttribute id="Department" transparentFilter="false"/>
        <viewAttribute id="PR_Combination" keepFlag="true" transparentFilter="false"/>
      </viewAttributes>
      <calculatedViewAttributes>
        <calculatedViewAttribute datatype="DATE" id="PR_Apr_Date" expressionLanguage="COLUMN_ENGINE">
          <formula>&quot;PR_Approved_Date&quot;</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="INTEGER" id="Days_for_RFQ_Creation" expressionLanguage="COLUMN_ENGINE">
          <formula>if(not(isnull(&quot;RFQ_Creation_Date&quot;)),daysbetween(&quot;PR_Apr_Date&quot;,&quot;RFQ_Creation_Date&quot;),&#xD;
if(isnull(&quot;RFQ_Creation_Date&quot;) and (not(isnull(&quot;Tech_Eval_Date&quot;))),0,&#xD;
if(isnull(&quot;RFQ_Creation_Date&quot;) and (not(isnull(&quot;Commer_Eval_Date&quot;))),0,&#xD;
if(isnull(&quot;RFQ_Creation_Date&quot;) and (not(isnull(&quot;PO_Creation_Date&quot;))),0,&#xD;
daysbetween(&quot;PR_Apr_Date&quot;,now())))))</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="INTEGER" id="Days_for_Tech_Eval" expressionLanguage="COLUMN_ENGINE">
          <formula>if(not(isnull(&quot;RFQ_Creation_Date&quot;)) and (not(isnull(&quot;Tech_Eval_Date&quot;))) ,daysbetween(&quot;RFQ_Creation_Date&quot;,&quot;Tech_Eval_Date&quot;),&#xD;
if(isnull(&quot;RFQ_Creation_Date&quot;) and (not(isnull(&quot;Tech_Eval_Date&quot;))) ,daysbetween(&quot;PR_Apr_Date&quot;,&quot;Tech_Eval_Date&quot;),&#xD;
if(not(isnull(&quot;RFQ_Creation_Date&quot;)) and isnull(&quot;Tech_Eval_Date&quot;) and (not(isnull(&quot;Commer_Eval_Date&quot;)) or (not(isnull(&quot;PO_Creation_Date&quot;)))),0,&#xD;
if(not(isnull(&quot;RFQ_Creation_Date&quot;)) and isnull(&quot;Tech_Eval_Date&quot;) and isnull(&quot;Commer_Eval_Date&quot;) and isnull(&quot;PO_Creation_Date&quot;),daysbetween(&quot;RFQ_Creation_Date&quot;,now()),0))))&#xD;
</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="INTEGER" id="Days_for_Commer_Eval" expressionLanguage="COLUMN_ENGINE">
          <formula>if(not(isnull(&quot;Tech_Eval_Date&quot;)) and (not(isnull(&quot;Commer_Eval_Date&quot;))) ,daysbetween(&quot;Tech_Eval_Date&quot;,&quot;Commer_Eval_Date&quot;),&#xD;
if(isnull(&quot;Tech_Eval_Date&quot;) and (not(isnull(&quot;Commer_Eval_Date&quot;))) and (not(isnull(&quot;RFQ_Creation_Date&quot;))),daysbetween(&quot;RFQ_Creation_Date&quot;,&quot;Commer_Eval_Date&quot;),&#xD;
if(isnull(&quot;Tech_Eval_Date&quot;) and (not(isnull(&quot;Commer_Eval_Date&quot;))) and isnull(&quot;RFQ_Creation_Date&quot;),daysbetween(&quot;PR_Apr_Date&quot;,&quot;Commer_Eval_Date&quot;),&#xD;
if(isnull(&quot;Commer_Eval_Date&quot;) and (not(isnull(&quot;PO_Creation_Date&quot;))),0,&#xD;
if(isnull(&quot;Commer_Eval_Date&quot;) and isnull(&quot;PO_Creation_Date&quot;),daysbetween(&quot;Commer_Eval_Date&quot;,now()),0)))))</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="INTEGER" id="Days_for_PO_Creation" expressionLanguage="COLUMN_ENGINE">
          <formula>if((not isnull(&quot;PO_Creation_Date&quot;)) and (not isnull(&quot;Commer_Eval_Date&quot;)),daysbetween(&quot;Commer_Eval_Date&quot;,&quot;PO_Creation_Date&quot;),&#xD;
if((not isnull(&quot;PO_Creation_Date&quot;)) and  isnull(&quot;Commer_Eval_Date&quot;) and leftstr(&quot;EBELN&quot;,2) = '42',daysbetween(&quot;PR_Apr_Date&quot;,&quot;PO_Creation_Date&quot;),&#xD;
if((not isnull(&quot;PO_Creation_Date&quot;)) and  isnull(&quot;Commer_Eval_Date&quot;) and leftstr(&quot;EBELN&quot;,2) != '42',0,&#xD;
if(isnull(&quot;EBELN&quot;) and  (not isnull(&quot;Commer_Eval_Date&quot;)) ,daysbetween(&quot;Commer_Eval_Date&quot;,now()),0))))&#xD;
</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="INTEGER" id="Days_for_PO_Approval" expressionLanguage="COLUMN_ENGINE">
          <formula>if((not isnull(&quot;PO_Creation_Date&quot;)) and isnull(&quot;PO_Approval_Date&quot;) and (not isnull(&quot;First_Delivery_Date&quot;)),0 ,&#xD;
if((not isnull(&quot;PO_Creation_Date&quot;)) and isnull(&quot;PO_Approval_Date&quot;) and isnull(&quot;First_Delivery_Date&quot;) ,daysbetween(&quot;PO_Creation_Date&quot;,now()),&#xD;
daysbetween(&quot;PO_Creation_Date&quot;,&quot;PO_Approval_Date&quot;)))&#xD;
</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="INTEGER" id="Days_for_First_Delivery" expressionLanguage="COLUMN_ENGINE">
          <formula>if((not isnull(&quot;PO_Approval_Date&quot;)) and isnull(&quot;First_Delivery_Date&quot;),daysbetween(&quot;PO_Approval_Date&quot;,now()),&#xD;
if((not isnull(&quot;First_Delivery_Date&quot;)) and isnull(&quot;PO_Approval_Date&quot;),daysbetween(&quot;PO_Creation_Date&quot;,&quot;First_Delivery_Date&quot;),&#xD;
daysbetween(&quot;PO_Approval_Date&quot;,&quot;First_Delivery_Date&quot;)))&#xD;
</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="PO_Type_Filter" length="20" expressionLanguage="COLUMN_ENGINE">
          <formula>if(strlen(&quot;Material_Service&quot;)>1,'Material','Service')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="Line_count" length="50" expressionLanguage="COLUMN_ENGINE">
          <formula>&quot;PR_Combination&quot;+&quot;EBELN&quot;+&quot;EBELP&quot;</formula>
        </calculatedViewAttribute>
