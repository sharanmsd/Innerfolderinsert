        <mapping xsi:type="Calculation:AttributeMapping" target="YTD_P_A" source="YTD_P_A"/>
        <mapping xsi:type="Calculation:AttributeMapping" target="List_1" source="List"/>
      </input>
      <filter>(in (&quot;List_1&quot;,'  Current Liabilities','  Non Current Liabilities','  Equity'))</filter>
    </calculationView>
