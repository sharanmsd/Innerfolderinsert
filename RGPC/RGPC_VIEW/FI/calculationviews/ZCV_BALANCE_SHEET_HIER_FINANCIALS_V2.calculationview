          <formula>if(&quot;List&quot; = 'Assets','01',if(&quot;List&quot; = '  Current Assets','02',if(&quot;List&quot; = '  Non Current Assets','03',if(&quot;List&quot; = '    Property,Plant and Equipment','04',if(&quot;List&quot;='    Net Investment In Finance Lease','05',if(&quot;List&quot;='Total Assets','06',if(&quot;List&quot;='Liabilities and Equity','07',if(&quot;List&quot;='  Current Liabilities','08',if(&quot;List&quot;='  Non Current Liabilities','09',if(&quot;List&quot;='    Term Loan','10',if(&quot;List&quot;='    Derivative Liability','11',if(&quot;List&quot;='    Lease Liability','12',if(&quot;List&quot;='    EOS Provision','13',if(&quot;List&quot;='  Equity','14',if(&quot;List&quot;='Total Liabilities and Equity','15','00')))))))))))))))&#xD;
</formula>
        </calculatedViewAttribute>
